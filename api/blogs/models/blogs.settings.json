{
  "connection": "default",
  "collectionName": "blogs",
  "info": {
    "name": "Blogs"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "cover": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "comments": {
      "collection": "comments"
    },
    "tags": {
      "collection": "tags"
    },
    "blog_categories": {
      "collection": "blog-categories"
    },
    "names": {
      "collection": "translates"
    },
    "descriptions": {
      "collection": "translates"
    }
  }
}
