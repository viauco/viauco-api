{
  "connection": "default",
  "collectionName": "products",
  "info": {
    "name": "Products"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "images": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "categories": {
      "via": "product",
      "collection": "categories"
    }
  }
}
