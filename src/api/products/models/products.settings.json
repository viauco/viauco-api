{
  "connection": "default",
  "collectionName": "products",
  "info": {
    "name": "Products"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "names": {
      "collection": "translates"
    },
    "descriptions": {
      "collection": "translates"
    },
    "images": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "categories": {
      "collection": "categories"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "colors": {
      "collection": "colors"
    },
    "likes": {
      "collection": "likes"
    },
    "ratings": {
      "collection": "rating"
    },
    "reviews": {
      "collection": "reviews"
    },
    "price": {
      "type": "decimal"
    },
    "discount": {
      "type": "decimal"
    },
    "amount": {
      "type": "decimal"
    },
    "tags": {
      "collection": "tags"
    },
    "sizes": {
      "collection": "sizes"
    }
  }
}
